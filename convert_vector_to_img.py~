from gensim.models import word2vec
import numpy as np

# max:9.400702 min:-10.083586

new_max = 255
new_min = 0
old_max = 9.400702
old_min = -10.083586

index = 0

model = word2vec.Word2Vec.load("masuda.model_2")
vocab = model.wv.vocab
with open('./entries/entry_' + str(index)) as f:
    words = f.read()
words = words.split()
for w in words:
    old_val = model[w]
    new_val = (new_max - new_min) * (old_val - old_min) / (old_max - old_min) + new_min
    print(new_val)
# # for v in vocab:
